<Application x:Class="Delegates_Events.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Delegates_Events"
             xmlns:vm="clr-namespace:Delegates_Events.ViewModels"
             xmlns:v="clr-namespace:Delegates_Events.Views"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:AccountViewModel}">
            <v:AccountView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:LogViewModel}">
            <v:LogView/>
        </DataTemplate>
    </Application.Resources>
</Application>
